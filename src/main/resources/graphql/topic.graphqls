type Topic{
    id:String
    name:String
}

extend type Topic{
    latestMessage:LatestMessage
}

extend type Topic{
    messages:[Message!]!
}

type LatestMessage{
    id:ID!,
    type:String!
    text:String!
    title:String!
    sendAt:Long!,
}

extend type Query{
    listTopic(token:String!):[Topic!]!
    topicDetail(token:String!,topicId:String):Topic!
}

extend type Mutation {
    createTopic(id:ID!,name:String!,token:String!):Topic!
    deleteTopic(id:ID!,token:String!):[Topic!]!
}